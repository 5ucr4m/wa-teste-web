{
  "name": "waproject-base-front",
  "description": "Front Base for WaProject",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "waproject-repository"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "yarn eslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "docker:release": "yarn docker:build && yarn docker:push",
    "docker:build": "docker build -t waproject/front-base -f docker/prod/Dockerfile .",
    "docker:push": "docker push waproject/front-base",
    "webpack-analyze": "node ./scripts/webpack-analyze.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently -r \"yarn lint\" \"yarn tsc --noEmit\"",
      "post-merge": "yarn"
    }
  },
  "dependencies": {
    "@material-ui/core": "4.1.2",
    "@react-form-fields/material-ui": "2.0.0",
    "axios": "0.19.0",
    "date-fns": "2.0.0-alpha.21",
    "lodash": "4.17.11",
    "mdi-react": "5.4.0",
    "query-string": "6.8.1",
    "raven-js": "3.27.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-image-cropper": "1.3.0",
    "react-jss": "8.6.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "react-swipeable-views": "0.13.3",
    "recharts": "1.6.2",
    "rxjs": "6.5.2",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.134",
    "@types/luxon": "1.15.1",
    "@types/node": "12.0.10",
    "@types/query-string": "6.3.0",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.4",
    "@types/react-swipeable-views": "0.13.0",
    "@types/recharts": "1.1.17",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "concurrently": "4.1.0",
    "eslint-config-react-app": "4.0.1",
    "husky": "2.5.0",
    "inquirer": "6.4.1",
    "ora": "3.4.0",
    "rimraf": "2.6.3",
    "typescript": "3.5.2",
    "webpack-bundle-analyzer": "3.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}